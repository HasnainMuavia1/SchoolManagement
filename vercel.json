
{
  "version": 2,
  "builds": [
    {
      "src": "api/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    { "src": "/", "has": [{ "type": "host", "value": "aktiportal.vercel.app" }], "status": 308, "headers": { "Location": "/management/" } },
    { "src": "/management/?", "has": [{ "type": "host", "value": "aktipos.vercel.app" }], "status": 308, "headers": { "Location": "/" } },
    {
      "src": "/(.*)",
      "dest": "api/wsgi.py"
    }
  ]
}